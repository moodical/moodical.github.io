    "react-webcam": "^5.2.3",
    "wait-on": "^6.0.1",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "react-scripts start",
    "local-build": "react-scripts build",

{
  "name": "moodical",
  "homepage": "https://moodical.github.io",
  "main": "./electron/main.js",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "concurrently": "^7.2.2",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jinke-music-player": "^4.24.1",
